# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Marvel' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  plugin 'cocoapods-keys', {
    :project => "Marvel",
    :target => "Marvel",
    :keys => [
      "MarvelApiKey",
      "MarvelPrivateKey"
    ]}

  # Pods for Marvel
   pod 'SwiftGen'
   pod 'CryptoSwift' 
   pod 'SDWebImage'
   pod "Reusable"
end
